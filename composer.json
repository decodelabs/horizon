{
    "name": "decodelabs/horizon",
    "description": "Simple HTML view containers",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.4",
        "decodelabs/coercion": "^0.3.5",
        "decodelabs/exceptional": "^0.6.3",
        "decodelabs/monarch": "^0.2.1",
        "decodelabs/nuance": "^0.1.9",
        "decodelabs/slingshot": "^0.3",
        "decodelabs/tagged": "^0.21"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7.0",
        "decodelabs/greenleaf": "^0.10",
        "decodelabs/harvest": "^0.7"
    },
    "conflict": {
        "decodelabs/greenleaf": "<0.10",
        "decodelabs/harvest": "<0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Horizon\\": "src/Horizon",
            "DecodeLabs\\Harvest\\Transformer\\DecodeLabs\\Horizon\\": "src/Harvest/Transformer",
            "DecodeLabs\\Greenleaf\\PageAction\\": "src/Greenleaf/PageAction",
            "DecodeLabs\\Tagged\\": "src/Tagged"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.5.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
